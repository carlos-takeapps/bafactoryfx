@model BAFactory.Fx.Security.MembershipProvider.Organization
@{
    ViewBag.Title = "Eliminar Organización";
}
@Html.Partial("~/Areas/Membership/Views/Controls/ContentTitle.cshtml")
<h3>
    Está seguro de que quiere eliminar esta Organización del sistema?</h3>

@using (Html.BeginForm())
{
    <fieldset>
        <legend>Organización</legend>
        <table width="100%">
            <tr>
                <td>
                    <div class="display-label">
                        Name</div>
                </td>
                <td>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.Name)
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div class="display-label">
                        Description</div>
                </td>
                <td>
                    <div class="display-field">
                        @Html.DisplayFor(model => model.Description)
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="2">
                    <div style="text-align: right;">
                        <input type="submit" value="Borrar" /></div>
                </td>
            </tr>
        </table>
    </fieldset>
  
}
<br />
@Html.Partial("~/Areas/Membership/Views/Controls/BackToIndexActionLink.cshtml")
